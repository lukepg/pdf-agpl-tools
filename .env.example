# pdf-agpl-tools Environment Configuration
# Copy this file to .env and modify as needed

# API Authentication (optional, leave empty to disable)
AGPL_API_KEY=your-secret-api-key

# File size limits
AGPL_MAX_FILE_SIZE_MB=100

# Timeouts (in seconds)
AGPL_REQUEST_TIMEOUT_SECONDS=120
AGPL_COMPRESSION_TIMEOUT_SECONDS=300

# Service info (for AGPL compliance)
AGPL_SOURCE_CODE_URL=https://github.com/lukepg/pdf-agpl-tools
